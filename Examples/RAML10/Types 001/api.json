{
    "types": {
        "Address": {
            "example": {
                "country": "US",
                "city": "SF",
                "street": "Geary Street",
                "house": "77, Suite 400",
                "zip": 94108
            },
            "properties": {
                "country": {
                    "enum": [
                        "US",
                        "ARG",
                        "RUS",
                        "GB"
                    ]
                },
                "city": {},
                "street": {},
                "house": {},
                "zip": {
                    "type": "number"
                }
            }
        },
        "Email": {
            "example": "press@mulesoft.com",
            "minLength": 15,
            "maxLength": 64,
            "pattern": "/[a-zA-Z0-9\\.]{1-32}@[a-zA-Z0-9]+\\.com/"
        },
        "User": {
            "examples": [
                {
                    "id": "11",
                    "name": "John Johnson",
                    "email": "press@mulesoft.com",
                    "phone": "+7-913-111-1111",
                    "address": {
                        "country": "RUS",
                        "city": "Novosibirsk",
                        "zip": "630090"
                    }
                },
                {
                    "id": "12",
                    "name": "John Johnson",
                    "email": "press1@mulesoft.com",
                    "phone": "+1-222-222-2222",
                    "address": {
                        "country": "US",
                        "city": "SF",
                        "zip": "123456"
                    }
                }
            ],
            "facets": {
                "role": {
                    "default": "user",
                    "enum": [
                        "user",
                        "manager",
                        "admin",
                        "super-admin"
                    ]
                }
            },
            "properties": {
                "id": {
                    "type": "number"
                },
                "name": {},
                "email": {
                    "type": "Email"
                },
                "phone": {
                    "minLength": 15,
                    "maxLength": 15,
                    "pattern": "/+\\d{1}-\\d{3}-\\d{3}-\\d{4}/"
                },
                "address": {
                    "type": "Address"
                }
            }
        },
        "Manager": {
            "type": "User",
            "examples": {
                "manager1": {
                    "id": "11",
                    "name": "John Johnson",
                    "email": "press@mulesoft.com",
                    "phone": "+7-913-111-1111",
                    "address": {
                        "country": "RUS",
                        "city": "Novosibirsk",
                        "zip": "630090"
                    }
                },
                "manager2": {
                    "id": "12",
                    "name": "John Johnson",
                    "email": "press1@mulesoft.com",
                    "phone": "+1-222-222-2222",
                    "address": {
                        "country": "US",
                        "city": "SF",
                        "zip": "123456"
                    }
                }
            },
            "fixedFacets": {
                "role": "manager"
            }
        },
        "Admin": {
            "type": "User",
            "fixedFacets": {
                "role": "admin"
            }
        },
        "SuperAdmin": {
            "type": "Admin"
        },
        "Team": {
            "properties": {
                "id": {
                    "type": "number"
                },
                "title": {
                    "pattern": "/[A-Z][a-z]*/"
                },
                "manager": {
                    "type": "Manager"
                },
                "admin": {
                    "type": "Admin|SuperAdmin"
                },
                "members": {
                    "type": "User[]"
                },
                "info": {
                    "properties": {
                        "address": {
                            "type": "Address"
                        },
                        "email": {
                            "type": "Email"
                        },
                        "size": {
                            "type": "number"
                        }
                    }
                }
            }
        }
    },
    "title": "TCK Sample. RAML 1.0 Types 001.",
    "version": "v1",
    "baseUri": "http://api.samplehost.com/{version}",
    "protocols": [
        "HTTP"
    ],
    "resources": [
        {
            "relativeUri": "/teams",
            "relativeUriPathSegments": [
                "teams"
            ],
            "methods": [
                {
                    "responses": {
                        "200": {
                            "body": {
                                "application/xml": {
                                    "type": "Team[]"
                                }
                            }
                        }
                    },
                    "method": "get"
                },
                {
                    "description": "create new team with desired title",
                    "body": {
                        "applciation/json": {
                            "properties": {
                                "title": {
                                    "pattern": "[A-Z][a-z]*"
                                }
                            }
                        }
                    },
                    "responses": {
                        "204": {
                            "body": {
                                "application/json": {
                                    "type": "User"
                                }
                            }
                        }
                    },
                    "method": "post"
                }
            ],
            "resources": [
                {
                    "relativeUri": "/{id}",
                    "relativeUriPathSegments": [
                        "{id}"
                    ],
                    "uriParameters": {
                        "id": {}
                    },
                    "methods": [
                        {
                            "responses": {
                                "200": {
                                    "body": {
                                        "application/json": {
                                            "type": "Team"
                                        }
                                    }
                                },
                                "404": {
                                    "body": {
                                        "text/plain": null
                                    }
                                }
                            },
                            "method": "get"
                        }
                    ]
                }
            ]
        },
        {
            "relativeUri": "/users",
            "relativeUriPathSegments": [
                "users"
            ],
            "methods": [
                {
                    "responses": {
                        "200": {
                            "body": {
                                "application/json": {
                                    "type": "User[]"
                                }
                            }
                        }
                    },
                    "method": "get"
                }
            ]
        },
        {
            "relativeUri": "/users/{id}",
            "relativeUriPathSegments": [
                "users",
                "{id}"
            ],
            "uriParameters": {
                "id": {}
            },
            "methods": [
                {
                    "responses": {
                        "200": {
                            "body": {
                                "application/json": {
                                    "type": "User"
                                }
                            }
                        }
                    },
                    "method": "get"
                }
            ]
        }
    ],
    "baseUriParameters": {
